<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:p="http://www.springframework.org/schema/p"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
<!-- Declare custom init and destroy methods-->
<bean class="com.springcore.collections.Emp" name="emp1">
<property name="name" value="Radheshyam"/>
<property name="phones">
  <list>
  <value>234455</value>
  <value>456767777</value>
  <value>989892566</value>
  <value>78923456067</value>

</list>

</property>
<property name="addresses">
<set>
<value>Delhi</value>
<value>Lucknow</value>
<value>Dehradun</value>
<value>Agra</value>
</set>
</property>
<property name="courses">
<map>
<entry key="java" value="1month"></entry>
<entry key="python" value="2month"></entry>
<entry key="javascript" value="3month"></entry>
<entry key="Html" value="1month"></entry>

</map>


</property>



</bean>
</beans>